        totalRequestString = totalRequestString + `&totalDiscountAmountStr=${totalDiscountAmount}`
        totalRequestString = totalRequestString + `&totalInvoiceCountStr=${totalInvoiceCount}`
        totalRequestString = totalRequestString + `&totalNonInvoiceCountStr=${totalNonInvoiceCount}`
        totalRequestString = totalRequestString + `&totalDiscountCountStr=${totalDiscountCount}`
        totalRequestString = totalRequestString + `&totalTaxAmountStr=${totalTaxAmount}`
